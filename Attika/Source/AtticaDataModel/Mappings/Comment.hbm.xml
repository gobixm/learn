<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Infotecs.Attika.AtticaDataModel"
                   namespace="Infotecs.Attika.AtticaDataModel" schema="dbo">
  <class name="Comment" table="Comments">
    <id name="Id">
      <generator class="guid" />
    </id>
    <property name="Text" />
    <property name="Created" />
    <many-to-one class="Article" name="ArticleId" />
  </class>
</hibernate-mapping>